<h1>Listing Authors of your Paper Management System </h1>
 
<table id = "authors">
    <tr>
        <th>Name</th>
        <th>Homepage</th>
        <th></th>
    </tr>
    <% @authors.each do |author| %>
    <tr>
        <td><%= author.name %></td>
        <td><%= author.homepage %></td>
        <td><%= link_to 'Show', author_path(author) %></td>
    </tr>
    <% end %>
    <td><%= link_to 'New', new_author_path %></td>
</table>